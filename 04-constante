//criando constantes com "const" (conts nome constante = <valor>)
//atividade de calculo do comprimento de uma circunferencia 

const pi = 3.14159265359
var raio = 5

console.log("comprimento de uma circunferencia de raio = "+ raio + "cm =", 2*pi*raio)

console.log("tentando alterar o valor de pi!")
// pi=10

